{
    "manifest_version": 2,
    "name": "DisTab",
    "description": "'New Tab' Theme + Opens a webpage as a popup!",
    "version": "1.1.0",
    "icons": {"128": "icon_128.png"},
    "background": {
        "scripts": [
            "main.js"
        ]
    },
    "permissions": [
        "tabs"
    ],
    "browser_action": {
       
    },
	"chrome_url_overrides" : {
		"newtab": "newtab\\page.html"
	}
}